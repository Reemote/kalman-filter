{
  "name": "kalman-filter",
  "version": "1.0.0",
  "description": "Kalman filter (and Extended Kalman Filter) Multi-dimensional implementation in Javascript",
  "main": "index.js",
  "scripts": {
    "test": "xo && ava",
    "semantic-release": "semantic-release",
    "build-demo": "browserify -d -r ./demo/src/main.js:main > docs/demo.js",
    "serve-demo": "http-server docs/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/piercus/kalman-filter.git"
  },
  "release": {
    "branches": [
      "master"
    ]
  },
  "keywords": [
    "kalman",
    "filter",
    "kalmanfilter",
    "kalman-filter",
    "extended",
    "extended-kalman-filter",
    "extended-kalmanfilter",
    "extended-kalman",
    "extended-filter",
    "constant",
    "speed",
    "acceleration",
    "dynamic",
    "model",
    "tracking",
    "math",
    "smoothing",
    "multi-dimension",
    "noise",
    "linear",
    "dynamic",
    "models",
    "forward",
    "smoothing",
    "online",
    "forward-backward",
    "smoothing",
    "autocovariance",
    "correlation",
    "covariance"
  ],
  "author": "Pierre Colle <piercus@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/piercus/kalman-filter/issues"
  },
  "homepage": "https://github.com/piercus/kalman-filter#readme",
  "devDependencies": {
    "ava": "^3.8.2",
    "browserify": "^16.5.2",
    "cholesky": "^1.2.0",
    "hasard": "^1.6.1",
    "semantic-release": "^17.1.1",
    "xo": "^0.32.0"
  },
  "dependencies": {
    "matrix-inverse": "^1.0.1"
  },
  "xo": {
    "ignores": [
      "./docs/**"
    ],
    "rules": {
      "no-multi-assign": 0,
      "unicorn/no-reduce": 0,
      "import/no-unresolved": 0,
      "ava/prefer-async-await": 0,
      "ava/no-skip-test": 1,
      "camelcase": [
        0,
        {
          "ignoreDestructuring": true
        }
      ],
      "valid-jsdoc": [
        1,
        {
          "requireParamDescription": false,
          "requireReturnDescription": false
        }
      ]
    }
  }
}
